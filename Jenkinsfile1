{
    agent any
   // tools
    //{
       // maven 'maven'
   // }
     stages
     {
          stage("checkout")
          {
              steps
              {
                  git 'https://github.com/spring-projects/spring-petclinic'
              }
          }
          stage("mvn-compile")
          {
              steps
              {
              sh "mvn compile"

              }
          }
     }
}
